{
  "include": [
    "**/*",
    "**/.server/**/*",
    "**/.client/**/*",
    ".react-router/types/**/*"
  ],
  "exclude":
  [
    "./build/**/*"
  ],
  "compilerOptions": {
    "lib": ["ES2023"],
    "types": [
      "vitest/globals",
      "@testing-library/jest-dom",
      "vitest",
    ],
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "rootDirs": [".", "./.react-router/types"],
    "baseUrl": ".",
    "allowImportingTsExtensions": true,
    "moduleDetection": "force",
    "noEmit": true,
  }
}
